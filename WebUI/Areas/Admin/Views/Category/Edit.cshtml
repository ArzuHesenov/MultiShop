@model Category

<div class="card shadow py-5">
<form method="post" class="col-6 m-auto my-3">
    <input type="text" class="form-control" value="" name="Name" placeholder="Category name"/>
    <input type="file" class="form-control" name="NewPhoto" placeholder="PhotoUrl"/>
    <input type="submit" class="btn btn-outline-success mt-5" value="Update"/>

</form>
</div>